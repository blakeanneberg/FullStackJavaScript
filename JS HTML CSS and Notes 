var HTMLbadges = prompt('how many HTML badges did you have?');
var CSSbadges = prompt('How many css badges did you have');
var totalBadges = parseInt(HTMLbadges) + parseInt(CSSbadges);
alert('wow! you ahve ' + totalBadges + 'badges!');

